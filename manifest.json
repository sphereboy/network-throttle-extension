{
  "manifest_version": 3,
  "name": "Network Throttle Controller",
  "version": "1.0",
  "description": "Control network throttling for testing and development",
  "permissions": ["debugger", "storage", "tabs", "notifications"],
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}
