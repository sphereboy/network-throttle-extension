{
  "name": "network-throttle-extension",
  "version": "1.0.0",
  "description": "Chrome extension to control network throttling for testing and development",
  "private": true,
  "scripts": {
    "build": "npx webpack --config webpack.config.js",
    "watch": "npx webpack --watch --config webpack.config.js",
    "dev": "npx webpack --mode development",
    "prod": "npx webpack --mode production",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "chrome-extension",
    "network-throttling",
    "development-tools"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@babel/preset-env": "^7.23.5",
    "babel-loader": "^9.1.3",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "html-webpack-plugin": "^5.5.3",
    "mini-css-extract-plugin": "^2.7.6",
    "style-loader": "^3.3.3",
    "terser-webpack-plugin": "^5.3.9",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  }
}
